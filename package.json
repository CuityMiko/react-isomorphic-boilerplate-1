{
  "name": "react-isomorphic-boilerplate",
  "version": "1.0.0",
  "description": "webpack3.*,react,ssr,isomorphic,es6",
  "main": "index.js",
  "author": "Candice",
  "license": "MIT",
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "ss": "webpack-dev-server --config ./tools/webpack.config.dev.js"
  },
  "betterScripts": {
    "start": {
      "command": "node ./dist/server/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config ./tools/webpack.config.prod.js --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "nodemon ./server/server.dev.js --watch server --watch tools",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "server"
      }
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
